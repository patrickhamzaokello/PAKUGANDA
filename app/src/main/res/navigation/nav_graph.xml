<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:label="@string/title_home"
        android:name="com.example.pakuganda.Fragments.Home"
        tools:layout="@layout/fragment_home" >
    </fragment>

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.pakuganda.Fragments.MapFragment"
        tools:layout="@layout/fragment_map"
        android:label="@string/analyze" />

    <fragment
        android:id="@+id/navigation_needs"
        android:name="com.example.pakuganda.Fragments.NeedsFragment"
        tools:layout="@layout/fragment_needs"
        android:label="@string/title_search"
        />

    <fragment
        android:id="@+id/navigation_schedule"
        android:name="com.example.pakuganda.Fragments.ScheduleFragment"
        tools:layout="@layout/fragment_schedule_fragment"
        android:label="@string/title_events" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.pakuganda.Fragments.SettingsFragment"
        tools:layout="@layout/fragment_settings"
        android:label="@string/title_library" />


</navigation>